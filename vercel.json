{
  "version": 2,
  "builds": [
    {
      "src": "src/main.server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/main.ts",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.server.ts"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Encoding",
          "value": "gzip"
        }
      ]
    }
  ]
}
